generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model GuestBook {
  id         Int    @id @default(autoincrement())
  name       String
  attendance String
  guestnum   String
  message    String
  createdAt  DateTime @default(now())
}
